        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView70" />

    <EditText
        android:id="@+id/editText31"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
