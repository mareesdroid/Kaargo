            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:backgroundTint="@color/googleBlue"
            android:text="Min"
            android:textAlignment="center"
            android:textAllCaps="false"

            android:textColor="#ffff"
            app:layout_constraintEnd_toStartOf="@+id/editText47"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView92" />

        <Button
            android:id="@+id/editText47"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"

            android:backgroundTint="@color/googleBlue"
            android:text="Max"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textColor="#ffff"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/editText46"
            app:layout_constraintTop_toBottomOf="@+id/textView92" />

        <Button
            android:id="@+id/button5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="8dp"
            android:background="@color/googleBlue"
